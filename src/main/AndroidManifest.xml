<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="monte.com.copaamerica2015mysql">

    <uses-permission android:name=
        "android.permission.INTERNET">
    </uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="copaamerica2015mysql"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FixtureActivity"
            android:label="@string/title_activity_fixture">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TeamsActivity"
            android:label="@string/title_activity_teams"></activity>
        <activity
            android:name=".GroupsActivity"
            android:label="@string/title_activity_groups"></activity>
    </application>

</manifest>
